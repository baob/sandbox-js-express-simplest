{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-extra-semi": 1,
      "semi": ["error", "always"]
    }
}
