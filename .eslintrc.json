{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true,
        "jest/globals": true
      },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "jest"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}
